{
    "presets": [
      ["@babel/preset-env", {
        "targets": {
          "node": "current",
          "browsers": [">0.25%, not dead"]
        }
      }],
      ["@babel/preset-react", {
        "runtime": "automatic"
      }]
    ],
    "plugins": [
      "@babel/plugin-transform-react-jsx",
      "@babel/plugin-proposal-private-methods",
      "@babel/plugin-transform-runtime",
      "@babel/plugin-syntax-jsx"
    ]
  }